{
  "build": {
    "env": {
      "PYTHONUNBUFFERED": "1"
    },
    "builder": "NIXPACKS",
    "buildCommand": "pip install -r todoproject/requirements.txt"
  },
  "deploy": {
    "startCommand": "python todoproject/manage.py runserver 0.0.0.0:$PORT",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
